POST {{baseUrl}}/api/servicos
Content-Type: application/json

{
    "clienteNome": "Cliente A",
    "endereco": "Rua A",
    "dataAgendamento": "2026-01-24T09:00:00",
    "valor": 100.00,
    "prestadorId": 8
}

