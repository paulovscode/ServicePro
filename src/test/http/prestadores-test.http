Content-Type: application/json

{
    "clienteNome": "Cliente 1",
    "clienteTelefone": "(11) 91111-1111",
    "endereco": "Rua A, 100",
    "dataAgendamento": "2026-01-25T10:00:00",
    "valor": 150.00,
    "prestadorId": 8
}

